    <!-- Strings shown in a dialog that appear when users try to refresh a certain kind of webpages -->
    <string name="mozac_feature_prompt_repost_title">Даҽазнык ари асаит ахь адыррақәа рышьҭра?</string>
    <string name="mozac_feature_prompt_repost_message">Ари адаҟьа арҿыцра аҵыхәтәантәи аус еиҭанаҳәар алшоит, иаҳҳәап, ашәара ма даҽазнык акомментари ашьҭрала.</string>
    <!-- Pressing this will dismiss the dialog and reload the page sending again the previous data -->
    <string name="mozac_feature_prompt_repost_positive_button_text">Даҽазнык идәықәҵатәуп адыррақәа</string>
    <!-- Pressing this will dismiss the dialog and not refresh the webpage -->
    <string name="mozac_feature_prompt_repost_negative_button_text">Мап ацәктәуп</string>

