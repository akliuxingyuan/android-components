    <string name="mozac_feature_prompts_suggest_strong_password_saved_snackbar_title">Амаӡажәа еиқәырхоуп</string>
    <!-- Title for showing the suggest strong password updated confirmation snackbar -->
    <string name="mozac_feature_prompts_suggest_strong_password_updated_snackbar_title">Амаӡажәа ԥсахуп</string>
    <!-- Title for showing the username updated confirmation snackbar -->
